{
  "name": "@n1k1t/mock-server",
  "version": "0.1.34",
  "description": "Powerful util to setup mocks over HTTP APIs",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "bin": "lib/bin/index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "start": "node lib/src/index",
    "start:dev": "NODE_OPTIONS=\"-r ts-node/register -r ./task-processor --no-warnings\" node test",
    "build": "rm -rf lib && npx tsc",
    "build:check": "npx tsc --noEmit",
    "prepare": "husky",
    "preversion": "npm run build:check && npm test",
    "version": "git add -A .",
    "postversion": "npm run build && git push && git push --tags"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/src/**/*.spec.ts"
    ],
    "setupFilesAfterEnv": [
      "jest-extended/all"
    ],
    "setupFiles": [],
    "transform": {
      ".+\\.ts?$": [
        "ts-jest",
        {
          "isolatedModules": true
        }
      ]
    }
  },
  "engines": {
    "node": ">=22.14.0"
  },
  "homepage": "https://github.com/n1k1t/mock-server",
  "author": "n1k1t",
  "license": "ISC",
  "devDependencies": {
    "@babel/plugin-transform-typescript": "7.19.3",
    "@babel/preset-env": "7.19.4",
    "@n1k1t/task-processor": "0.1.21",
    "@types/babelify": "7.3.7",
    "@types/commander": "2.12.2",
    "@types/express": "4.17.13",
    "@types/http-proxy": "1.17.9",
    "@types/jest": "29.5.11",
    "@types/lodash": "4.14.184",
    "@types/minimatch": "5.1.2",
    "@types/node": "22.13.4",
    "@types/node-gzip": "1.1.3",
    "@types/uuid": "8.3.4",
    "@types/ws": "8.5.13",
    "babelify": "10.0.0",
    "fnv1a": "1.1.1",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-extended": "4.0.2",
    "ts-jest": "29.1.1",
    "ts-node": "10.9.2",
    "tsify": "5.0.4",
    "typescript": "5.7.2"
  },
  "dependencies": {
    "@faker-js/faker": "9.2.0",
    "@n1k1t/typebox": "0.31.14",
    "axios": "0.27.2",
    "body-parser": "1.20.3",
    "colors": "1.4.0",
    "commander": "9.4.0",
    "dayjs": "1.11.13",
    "deepmerge": "4.3.1",
    "fast-xml-parser": "4.5.0",
    "handlebars": "4.7.7",
    "http-proxy": "1.18.1",
    "https-proxy-agent": "7.0.6",
    "ioredis": "5.4.1",
    "jsonpath-plus": "7.2.0",
    "lodash": "4.17.21",
    "minimatch": "5.1.0",
    "mongodb": "4.12.1",
    "node-gzip": "1.1.2",
    "random-animal-name": "0.1.1",
    "reflect-metadata": "0.2.1",
    "rfdc": "1.3.0",
    "rxjs": "7.8.1",
    "socket.io": "4.5.3",
    "socket.io-client": "4.5.4",
    "uuid": "9.0.0",
    "ws": "8.18.0"
  }
}
